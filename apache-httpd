

<VirtualHost *:80>
    ServerAdmin admin@k-vision.tv
    #Redirect permanent / https://sikovi.k-vision.tv/
    DocumentRoot /var/www/html/sikovi
    ServerName sikovinew.k-vision.tv
    ErrorLog logs/sikovi.k-vision.tv-error_log
    CustomLog logs/dummy-host.example.com-access_log common
    <Directory "/var/www/html/sikovi">
        LimitRequestBody 512000
        Options -Indexes FollowSymLinks +ExecCGI
        AllowOverride All
        Order allow,deny
        Allow from all
    </Directory>
</VirtualHost>



#httpd.conf

KeepAliveTimeout 5
MaxKeepAliveRequests 500
KeepAlive On

<IfModule prefork.c>
    StartServers 5
    MinSpareServers 5
    MaxSpareServers 10
    MaxClients 150
    MaxRequestsPerChild 3000
</IfModule>

